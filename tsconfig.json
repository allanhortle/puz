{
  "compilerOptions": {
    "moduleResolution": "nodenext",
    "module": "nodenext",
    "strict": true,
    "allowSyntheticDefaultImports": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "outDir": "out",
    "resolveJsonModule": true
  },
  "include": ["src"]
}
